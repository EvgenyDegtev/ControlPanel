@model List<ControlPanel.Models.Skill>

<h2>AddSkill</h2>

<table class="table table-hover">
	<tr>
		<th>Навык</th>
		<th>ID</th>
		<th>Алгоритм</th>
		<th></th>
	</tr>
	@foreach(var item in Model)
	{
		<tr>
			<td>
				@Html.DisplayFor(skill=>item.Name)
			</td>
			<td>
				@Html.DisplayFor(skill=>item.Key)
			</td>
			<td>
				@Html.DisplayFor(skill=>item.Algorithm)
			</td>
			<td>
				<a href=@Url.Action("SkillAddConfirmation", new { agentId = ViewBag.AgentId, skillId = item.Id })><i class="fa fa-plus-square"></i></a>
			</td>
		</tr>
	}
</table>



